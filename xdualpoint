# Dell DualPoint Keyboard Setup for scroll
INPUT_NAME="AlpsPS/2 ALPS DualPoint Stick"

xinput set-prop "${INPUT_NAME}" "Evdev Wheel Emulation" 1
xinput set-prop "${INPUT_NAME}" "Evdev Wheel Emulation Button" 2
xinput set-prop "${INPUT_NAME}" "Evdev Wheel Emulation Timeout" 200
xinput set-prop "${INPUT_NAME}" "Evdev Wheel Emulation Axes" 6 7 4 5
xinput set-prop "${INPUT_NAME}" "Device Accel Constant Deceleration" 0.95
